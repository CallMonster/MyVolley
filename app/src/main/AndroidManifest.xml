<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tj.myvolley">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:name=".base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.JsonObjectRequestActivity" />
        <activity android:name=".activity.ImageRequestActivity"></activity>
        <activity android:name=".activity.XMLRequestActivity"></activity>
        <activity android:name=".activity.GsonRequestActivity"></activity>
        <activity android:name=".activity.CacheRequestActivity"></activity>
        <activity android:name=".activity.ListViewActivity"></activity>
        <activity android:name=".activity.MultipartActivity"></activity>
        <activity android:name=".activity.StringRequestActivity"></activity>
        <activity android:name=".activity.SSLRequestActivity"></activity>
        <activity android:name=".activity.StatusJsonActivity"></activity>
    </application>

</manifest>